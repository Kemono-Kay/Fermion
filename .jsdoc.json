{
  "opts": {
      "destination": "./docs/jsdoc/",
  "recurse": true
  },
"source": {
      "include": [
          "./app/"
      ],
      "exclude": [
        "node_modules",
        "local",
        "lib"
      ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  }
}